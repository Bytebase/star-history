{
  "manifest_version": 2,
  "author": "bytebase",
  "homepage_url": "https://github.com/bytebase/star-history-plugin",
  "name": "Star History",
  "description": "A chrome extension showing star history graph of current GitHub repository",
  "version": "1.5",
  "icons": {
    "128": "assets/ico.png"
  },
  "browser_action": {
    "default_icon": "./assets/ico.png",
    "default_popup": "popup.html"
  },
  "permissions": ["tabs", "activeTab", "https://api.github.com/"]
}
